fizzbuzz: for i in range 1 100 (
    if and equals 0 mod i 3 equals 0 mod i 5
    (
        printLn "FizzBuzz"
    ) else (
        if equals 0 mod i 3 (
            printLn "Fizz"
        ) else (
            if equals 0 mod i 5 (
                printLn "Buzz"
            ) else (
                printLn i
            )
        )
    )
)
