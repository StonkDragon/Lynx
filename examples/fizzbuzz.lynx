fizzbuzz: string = for i in range 0 100 (
    if i ("\n")
    if and eq 0 mod i 3 eq 0 mod i 5 (
        "FizzBuzz"
    ) else (
        if eq 0 mod i 3 (
            "Fizz"
        ) else (
            if eq 0 mod i 5 (
                "Buzz"
            ) else (
                i
            )
        )
    )
)

(printLn fizzbuzz)
